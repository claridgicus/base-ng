{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/combined",
  "framework": null,
  "rewrites": [
    { "source": "/angular/:path*", "destination": "/angular/index.html" },
    { "source": "/((?!angular/).*)", "destination": "/index.html" }
  ]
}
