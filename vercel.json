{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/combined",
  "framework": null,
  "rewrites": [
    { "source": "/docs/:path*", "destination": "/docs/index.html" },
    { "source": "/((?!docs/).*)", "destination": "/index.html" }
  ]
}
